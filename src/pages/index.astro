---
import BaseLayout from "../layouts/BaseLayout.astro";
import DefaultLeft from '../components/DefaultLeft.astro';
import Homepage from '../components/Homepage.astro';
import { personalInfo } from "../data/personalInfo.ts";
import { credits } from "../data/credits.ts";
---

<BaseLayout>
    <div slot="left">
        {<DefaultLeft personalInfo={personalInfo}/>}
    </div>

    <div slot="right">
        {<Homepage credits={credits}/>}
    </div>
</BaseLayout>